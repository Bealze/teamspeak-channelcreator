{
  "name": "teamspeak-channelcreator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:teamcity": "eslint server --format node_modules/eslint-teamcity/index.js",
    "prettier": "prettier --config .prettierrc --write ./pages/**/*.vue ./components/**/*.vue ./layouts/**/*.vue ./middleware/**/*.js ./plugins/**/*.js ./mixins/**/*.js ./store/**/*.js ./server/**/*.js ./assets/**/*.scss ./assets/**/*.css ./*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SvenC56/teamspeak-channelcreator.git"
  },
  "author": "Sven Cieslok <mail@svencieslok.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SvenC56/teamspeak-channelcreator/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "eslint-teamcity": {
    "reporter": "inspections",
    "report-name": "My ESLint Violations",
    "error-statistics-name": "My ESLint Error Count",
    "warning-statistics-name": "My ESLint Warning Count"
  },
  "homepage": "https://github.com/SvenC56/teamspeak-channelcreator#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "node-cron": "^2.0.3",
    "nuxt": "^2.11.0",
    "path": "^0.12.7",
    "ts3-nodejs-library": "^2.3.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^1.10.2",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-teamcity": "^2.2.0",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
